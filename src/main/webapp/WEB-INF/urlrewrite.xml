<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
		"http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<!-- Config bestand voor UrlRewriteFilter http://www.tuckey.org/urlrewrite/ -->
<urlrewrite>
	<rule>
		<name>inbound param mapper</name>
		<note>mapping van url naar query params</note>
		<from>^/kaart/mapname/([a-z]+)$</from>
		<to>/kaart?mapname=$1</to>
	</rule>
</urlrewrite>